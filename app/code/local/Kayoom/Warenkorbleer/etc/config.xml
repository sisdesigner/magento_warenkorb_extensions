<?xml version="1.0"?>
<config>
    <modules>
        <Kayoom_Warenkorbleer>
            <version>0.0.1</version>
        </Kayoom_Warenkorbleer>
    </modules>
    <global>
        <helpers>
            <warenkorbleer>
                <class>Kayoom_Warenkorbleer_Helper</class>
            </warenkorbleer>
        </helpers>
        <models>
             <warenkorbleer>
                <class>Kayoom_Warenkorbleer_Model</class>
             </warenkorbleer>
        </models>
    </global>
    <adminhtml>
        <acl>
            <resources>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                        <warenkorbleer_options>
                                            <title>Custom Configuration Section</title>
                                        </warenkorbleer_options>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>
</config>